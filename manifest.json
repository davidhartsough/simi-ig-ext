{
  "manifest_version": 3,
  "name": "SiMi IG",
  "version": "1.0",
  "description": "Simplistic Minimalistic Instagram",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": ["simi-ig.js"],
      "matches": ["https://www.instagram.com/"]
    },
    {
      "css": ["simi-ig.css"],
      "matches": ["https://www.instagram.com/*"]
    }
  ]
}
